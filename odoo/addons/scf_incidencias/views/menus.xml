<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="scf_incidencias.acciones_issues" model="ir.actions.act_window">
            <field name="name">Incidencias</field>
            <field name="res_model">scf_incidencias.issues</field>
            <field name="view_mode">tree,form,kanban,calendar</field>
        </record>

        <record id="scf_incidencias.acciones_activos" model="ir.actions.act_window">
            <field name="name">Inventario de Activos</field>
            <field name="res_model">scf_incidencias.activos</field>
            <field name="view_mode">tree,form,kanban</field>
        </record>

        <record id="scf_incidencias.acciones_intervenciones" model="ir.actions.act_window">
            <field name="name">Partes de Trabajo</field>
            <field name="res_model">scf_incidencias.intervenciones</field>
            <field name="view_mode">tree,form,graph</field>
        </record>

        <record id="scf_incidencias.acciones_etiquetas" model="ir.actions.act_window">
            <field name="name">Etiquetas</field>
            <field name="res_model">scf_incidencias.etiquetas</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem
            id="scf_incidencias.root_menu"
            name="Gestor de Incidencias"
            sequence="1" />

        <menuitem
            id="scf_incidencias.issues_menu"
            name="Incidencias"
            parent="scf_incidencias.root_menu"
            action="scf_incidencias.acciones_issues"
            sequence="1" />

        <menuitem
            id="scf_incidencias.activos_menu"
            name="Activos"
            parent="scf_incidencias.root_menu"
            action="scf_incidencias.acciones_activos"
            sequence="2" />

        <menuitem
            id="scf_incidencias.intervenciones_menu"
            name="Intervenciones"
            parent="scf_incidencias.root_menu"
            action="scf_incidencias.acciones_intervenciones"
            sequence="3" />

        <menuitem
            id="scf_incidencias.etiquetas_menu"
            name="ConfiguraciÃ³n Etiquetas"
            parent="scf_incidencias.root_menu"
            action="scf_incidencias.acciones_etiquetas"
            sequence="4" />

    </data>
</odoo>