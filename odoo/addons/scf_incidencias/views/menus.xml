<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="scf_incidencias.acciones" model="ir.actions.act_window">
            <field name="name">Incidencias</field>
            <field name="res_model">scf_incidencias.issues</field> <field name="view_mode">tree,form</field>
        </record>

        <record id="scf_incidencias.acciones_activos" model="ir.actions.act_window">
            <field name="name">Inventario de Activos</field>
            <field name="res_model">scf_incidencias.activo</field> <field name="view_mode">tree,form</field>
        </record>


        <menuitem 
            id="scf_incidencias.root_menu" 
            name="Gestor Incidencias" 
            sequence="1"/>

        <menuitem 
            id="scf_incidencias.issues_menu" 
            name="Incidencias" 
            parent="scf_incidencias.root_menu" 
            action="scf_incidencias.acciones" 
            sequence="1"/>

        <menuitem 
            id="scf_incidencias.activos_menu" 
            name="Inventario" 
            parent="scf_incidencias.root_menu" 
            action="scf_incidencias.acciones_activos" 
            sequence="2"/>
            
    </data>
</odoo>